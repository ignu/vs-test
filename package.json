{
  "name": "vstest",
  "displayName": "VsTest",
  "description": "Run focused unit tests from the file you're editing",
  "version": "0.0.1",
  "publisher": "ignu",
  "engines": {
    "vscode": "^1.25.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.runFocusedTest"
  ],
  "main": "./out/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.runFocusedTest",
        "title": "VsTest: Run Focused Test"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "VsTest configuration",
      "properties": {
        "vstest.jest.command": {
          "description": "The path to the Jest binary, or an npm command to run tests prefixed with `--` e.g. `node_modules/.bin/jest` or `npm test --`",
          "type": "string",
          "default": "./node_modules/.bin/jest"
        },
        "vstest.jest.flags": {
          "description": "Flags to pass to jest",
          "type": "string",
          "default": "--no-coverage"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.6.1",
    "vscode": "^1.1.6",
    "tslint": "^5.8.0",
    "@types/node": "^7.0.43",
    "@types/mocha": "^2.2.42"
  }
}
