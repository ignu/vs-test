{
  "name": "vs-test",
  "displayName": "VSTest",
  "description": "Run focused unit tests from the file you're editing",
  "version": "0.0.1",
  "publisher": "ignu",
  "engines": {
    "vscode": "^1.25.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.runFocusedTest"
  ],
  "main": "./out/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.runFocusedTest",
        "title": "Run Focused Test"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.6.1",
    "vscode": "^1.1.6",
    "tslint": "^5.8.0",
    "@types/node": "^7.0.43",
    "@types/mocha": "^2.2.42"
  }
}
